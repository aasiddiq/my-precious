{
  "name": "libprecious-next",
  "version": "2.2.0",
  "description": "a local package archive, of my own",
  "main": "index.js",
  "files": [
    "*.js",
    "lib"
  ],
  "scripts": {
    "prerelease": "npm t",
    "postrelease": "npm publish && git push --follow-tags",
    "pretest": "standard",
    "release": "standard-version -s -t 'lib-v' -r minor",
    "test": "tap -J --nyc-arg=--all --coverage test/*.js",
    "update-coc": "weallbehave -o . && git add CODE_OF_CONDUCT.md && git commit -m 'docs(coc): updated CODE_OF_CONDUCT.md'",
    "update-contrib": "weallcontribute -o . && git add CONTRIBUTING.md && git commit -m 'docs(contributing): updated CONTRIBUTING.md'"
  },
  "repository": "https://github.com/zkat/my-precious",
  "keywords": [
    "npm",
    "package manager",
    "caching",
    "downloader",
    "local registry"
  ],
  "author": {
    "name": "Kat March√°n",
    "email": "kzm@sykosomatic.org",
    "twitter": "maybekatz"
  },
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.1",
    "detect-indent": "^5.0.0",
    "detect-newline": "^2.1.0",
    "find-npm-prefix": "^1.0.2",
    "graceful-fs": "^4.1.11",
    "lock-verify": "^2.0.2",
    "mkdirp": "^0.5.1",
    "npm-logical-tree": "^1.2.1",
    "npm-package-arg": "^6.1.0",
    "pacote": "^8.1.6",
    "protoduck": "^5.0.0",
    "rimraf": "^2.6.2",
    "ssri": "^6.0.2"
  },
  "devDependencies": {
    "get-stream": "^3.0.0",
    "has-unicode": "^2.0.1",
    "mississippi": "^3.0.0",
    "nock": "^9.4.1",
    "npmlog": "^4.1.2",
    "nyc": "^14.0.0",
    "require-inject": "^1.4.3",
    "standard": "^12.0.1",
    "standard-version": "^6.0.0",
    "tacks-next": "^1.3.1",
    "tap": "^12.0.1",
    "tar-stream": "^1.6.1",
    "weallbehave": "^1.2.0",
    "weallcontribute": "^1.0.8"
  },
  "config": {
    "nyc": {
      "exclude": [
        "node_modules/**",
        "test/**"
      ]
    }
  },
  "overrides": {
    "lock-verify": {
      "yargs": "^13.1.2"
    },
    "weallbehave": {
      "yargs": "^13.1.2"
    },
    "weallcontribute": {
      "yargs": "^13.1.2"
    }
  }
}
